{
    "version": 2,
    "builds": [
        {
            "src": "build/server.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "build/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/server.ts"
        }
    ]
}